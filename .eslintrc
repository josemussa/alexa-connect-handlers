{
    extends: [
        'airbnb-base',
    ],
    env: {
        node: true,
        mocha: true,
    },
    rules: {
        'no-use-before-define': 0,
        'import/prefer-default-export': 0,
        'import/no-unresolved': 0,
        'import/extensions': 0,
        'no-multiple-empty-lines': [2, {'max': 1}],
        'default-case': 0,
        'indent': [2, 4, { SwitchCase: 1 } ],
        'no-param-reassign': 0,
        'no-shadow': 0,
        'max-len': [2, 100, 4],
        'import/no-extraneous-dependencies': 0
    }
}
